<%- include("partials/header") -%>





<!-- Tab Container Buttons -->


<div class="container mt-2 post-container">
  <ul class="nav nav-tabs " id="myTab" role="tablist">
    <li class="nav-item" role="presentation">
      <button class="nav-link active btn btn-outline-primary" id="home-tab" data-bs-toggle="tab" data-bs-target="#home" type="button" role="tab" aria-controls="home" aria-selected="true">All Notices</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link btn btn-outline-primary" id="profile-tab" data-bs-toggle="tab" data-bs-target="#profile" type="button" role="tab" aria-controls="profile" aria-selected="false">Important</button>
    </li>
    <li class="nav-item" role="presentation">
      <button class="nav-link btn btn-outline-primary" id="contact-tab" data-bs-toggle="tab" data-bs-target="#contact" type="button" role="tab" aria-controls="contact" aria-selected="false">Assignments</button>
    </li>
  </ul>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane show active " id="home" role="tabpanel" aria-labelledby="home-tab">

      <!-- Tab Container 1 -->
      <% article.forEach(article => { %>

      <div class="container mt-4  p-4 post-container bx">
        <div class="container text-muted d-inline my-4">
          <p class="d-inline ml-4 meta-data"> <%= article.createdAt.toLocaleDateString('en-GB')  %> </p>
          <p class="d-inline meta-data"> <%= article.nType %></p>

        </div>
        <div class="container mt-4">
          <h3> <%= article.title %></h3>
        </div>

        <div class="container my-4">
          <p> <%= article.description %></p>
        </div>
        <div class="container">
          <p> <%= article.markdown %></p>
        </div>
      </div>
      <% }); %>



    </div>

    <!-- Tab Container 2 -->


    <div class="tab-pane fade " id="profile" role="tabpanel" aria-labelledby="profile-tab">
      <% important.forEach(important => { %>

      <div class="container mt-4 p-4 post-container bx">
        <div class="container text-muted d-inline">
          <p class="d-inline meta-data"> <%= important.createdAt.toLocaleDateString('en-GB')%> </p>
          <p class="d-inline meta-data"> <%= important.nType %></p>

        </div>
        <div class="container mt-4">
          <h3> <%= important.title %></h3>
        </div>

        <div class="container my-4">
          <p> <%= important.description %></p>
        </div>
        <div class="container">
          <p> <%= important.markdown %></p>
        </div>

      </div>
      <% }); %>

    </div>

    <!-- Tab Container 3 -->

    <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">

      <% assignments.forEach(assignments => { %>

      <div class="container mt-4  p-4 post-container bx">
        <div class="container text-muted d-inline meta">
          <span class="d-inline meta-data"> <%= assignments.createdAt.toLocaleDateString('en-GB') %> </span>
          <span class="d-inline meta-data"> <%= assignments.nType %></span>

        </div>
        <div class="container mt-4">
          <h3> <%= assignments.title %></h3>
        </div>

        <div class="container my-4">
          <p> <%= assignments.description %></p>
        </div>
        <div class="container">
          <p> <%= assignments.markdown %></p>
        </div>


      </div>
      <% }); %>


    </div>

  </div>

</div>










<%- include("partials/footer") -%>
