
<%- include("partials/header") -%>


<div class="container  pt-4 pb-4">
  <h1 id="timer" class="mb-6"></h1>
  <a href="/posts/new" class="btn btn-success"> New Article</a>

</div>



<% article.forEach(article => { %>

  <div class="container mt-4 border border-secondary p-4 post-container">
    <div class="container">
      <h3> <%= article.title %></h3>
    </div>
  <div class="container text-muted">
    <p> <%= article.createdAt.toLocaleDateString() %></p>
  </div>
    <div class="container">
      <p> <%= article.content %></p>
    </div>


  </div>


<% }); %>













<%- include("partials/footer") -%>
